package  {	import flash.display.MovieClip;		public class Main extends MovieClip	{		public function Main()		{			for( var i:int=0; i<20; i++ )						// adding Box sprites to stage.			{				var box : Box;				box	= new Box();								box.name = "box_" + i;				box.mouseEnabled = true;								box.x = Math.random() * stage.stageWidth;				box.y = Math.random() * stage.stageHeight;				box.rotation = Math.random() * 360;								stage.addChild( box );			}		}	}	}